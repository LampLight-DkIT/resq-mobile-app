{
  "expo": {
    "name": "resq-app",
    "slug": "resq-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/resq-color.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera for emergency documentation.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to provide emergency services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to provide emergency services.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload emergency photos.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone for emergency audio recording."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "175dcd88-9939-420c-a4e4-d6f8b9992c05"
      }
    },
    "owner": "adityasutar",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/175dcd88-9939-420c-a4e4-d6f8b9992c05"
    },
    "expo": {
      "name": "resq-app",
      "slug": "resq-app",
      "version": "1.0.0",
      "orientation": "portrait",
      "platforms": [
        "ios",
        "android"
      ],
      "android": {
        "package": "com.adityasutar.resqapp",
        "googleServicesFile": "./google-services.json"
      },
      "extra": {
        "eas": {
          "projectId": "175dcd88-9939-420c-a4e4-d6f8b9992c05"
        }
      },
      "newArchEnabled": true,
      "scheme": "Resq"
    }
  }
}